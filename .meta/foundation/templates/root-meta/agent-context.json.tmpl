{
  "directory": "{{DIRECTORY}}",
  "purpose": "{{PURPOSE}}",
  "scope": "{{SCOPE}}",
  "operational_boundaries": {
    "ai_can": [
      {{#AI_CAN}}
      "{{.}}"{{^LAST}},{{/LAST}}
      {{/AI_CAN}}
    ],
    "ai_requires_approval": [
      {{#AI_REQUIRES_APPROVAL}}
      "{{.}}"{{^LAST}},{{/LAST}}
      {{/AI_REQUIRES_APPROVAL}}
