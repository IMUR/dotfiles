  "key_files": [
    {{#KEY_FILES}}
    {
      "file": "{{FILE}}",
      "purpose": "{{PURPOSE}}",
      "editable": {{EDITABLE}}
    }{{^LAST}},{{/LAST}}
    {{/KEY_FILES}}
  ],
  "validation": {
    "commands": [
      {{#VALIDATION_COMMANDS}}
      "{{.}}"{{^LAST}},{{/LAST}}
      {{/VALIDATION_COMMANDS}}
    ],
    "required_before": "{{VALIDATION_REQUIRED_BEFORE}}"
  },
  "workflows": [
    {{#WORKFLOWS}}
    {
      "name": "{{NAME}}",
      "description": "{{DESCRIPTION}}",
      "steps": [
        {{#STEPS}}
        "{{.}}"{{^LAST}},{{/LAST}}
        {{/STEPS}}
      ]
    }{{^LAST}},{{/LAST}}
    {{/WORKFLOWS}}
  ],
  "_meta": {
    "generated": true,
    "generator": ".meta/whitelists/generate-meta-files.sh",
    "schema": ".meta/schemas/agent-context.schema.json",
    "last_generated": "{{GENERATED_DATE}}"
  }
}
